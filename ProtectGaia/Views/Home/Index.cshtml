                    </div>
                </div>
                <div class="col-md-7">
                    <div class="mu-video-left">
                        <a href="#" class="mu-video-play-btn"><i class="fa fa-play" aria-hidden="true"></i></a>
                    </div>
                </div>
            </div>

            <!-- Start Video Popup -->
            <div class="mu-video-popup">
                <div class="mu-video-iframe-area">
                    <a class="mu-video-close-btn" href="#"><i class="fa fa-times" aria-hidden="true"></i></a>
                    <iframe width="854" height="480" src="https://www.youtube.com/embed/8q7_aV8eLUE" allowfullscreen></iframe>
                </div>
            </div>
            <!-- End Video Popup -->

        </div>
