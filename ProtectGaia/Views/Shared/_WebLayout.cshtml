<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <title>Home: Ecomorph</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
    <link rel="stylesheet" href="~/assets/css/main.css" />

    <noscript><link rel="stylesheet" href="/css/noscript.css" /></noscript>

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>


    <title>@ViewBag.Title - Eco Morph</title>
    <!-- Favicon -->
    <link rel="shortcut icon" type="/image/icon" href="/favicon.ico" />
    <!-- Font Awesome -->
    @* <link href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">*@
    <link href="~/lib/font-awesome/css/all.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="/js/geoLocation.js"></script>

</head>
<body>

    <!--START SCROLL TOP BUTTON -->
    <a class="scrollToTop" href="#">
        <i class="fa fa-angle-up"></i>
    </a>
    <!-- END SCROLL TOP BUTTON -->
    <!-- Start Header -->
    <div id="page-wrapper">

        <!-- Header -->
        <section id="header">
            <!-- Logo -->
            <h1>
                <a asp-controller="Home" asp-action="Index">
                    <img src="~/images/logo.jpeg" alt="ecoMorph" style="width:4rem;height:4rem;" />
                </a>EcoMorph
            </h1>

            <!-- Nav -->
            <nav id="nav">
                <ul>
                    <li class="current"><a asp-controller="Home" asp-action="Index">Home</a></li>
                    <li>
                        <a href="#">Explainers</a>
                        <ul>
                            <li><a asp-controller="Explore" asp-Action="EnergyConsumption" onclick="MakeActive('EnergyConsumption')">Energy Consumption</a></li>
                            <li><a asp-controller="Explore" asp-Action="TransportEmissions" onclick="MakeActive('TransportEmissions')">Transport Emissions</a></li>
                            <li><a asp-controller="Explore" asp-Action="HouseholdPractices" onclick="MakeActive('HouseholdActivities')">Household Practices</a></li>
                            <li><a asp-controller="Explore" asp-Action="ClimateChange" onclick="MakeActive('ClimateChange')">Climate Change</a></li>
                            <li><a asp-controller="Explore" asp-Action="HealthImpacts" onclick="MakeActive('HealthImpacts')">Health Impacts</a></li>
                            <li><a asp-controller="Explore" asp-Action="MakeAChange" onclick="MakeActive('MakeAChange')">Make a Change</a></li>
                        </ul>
                    </li>
                    <li><a href="left-sidebar.html">TakeChallenge</a></li>
                    <li> <div class="deviceLocation"> Detecting . . .</div> </li>

                </ul>
            </nav>
        </section>
    </div>
    <main>
        <!-- End Header -->
        @RenderBody()
    </main>
    <!-- End main content -->
    <!-- Start footer -->

    <section id="footer" style="filter: brightness(100%);padding-bottom: 0.5px; padding-top: 0.5px;">
        <div class="container">
            <!-- Copyright -->
            <div id="copyright">
                <ul class="links">
                    <li>Designed by Environs. All rights reserved.</li>
                </ul>
            </div>
        </div>
    </section>


    <!-- End footer -->
    <!-- JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/js/bootstrap.min.js" integrity="sha384-h0AbiXch4ZDo7tp9hKZ4TsHbi047NrKGLO3SEJAg45jXxnGIfYzk4Si90RDIqNm1" crossorigin="anonymous"></script>
    <!-- Slick slider -->
    @* <script type="text/javascript" src="/js/slick.min.js"></script>*@
    <!-- Progress Bar -->
    <script src="https://unpkg.com/circlebars@1.0.3/dist/circle.js"></script>
    <!-- Filterable Gallery js -->
    <script type="text/javascript" src="/js/jquery.filterizr.min.js"></script>
    <!-- Gallery Lightbox -->
    <script type="text/javascript" src="/js/jquery.magnific-popup.min.js"></script>
    <script src="/js/contact.js" type="text/javascript"></script>
    <!-- Counter js
    <script type="text/javascript" src="/js/counter.js"></script>-->
    <!-- Ajax contact form  -->
    <script type="text/javascript" src="/js/app.js"></script>
    <script type="text/javascript" src="/js/header.js"></script>


    <!-- Custom js -->
    <script type="text/javascript" src="/js/custom.js"></script>

    <!--from new template-->
    <script src="assets/js/jquery.min.js"></script>
    <script src="assets/js/jquery.dropotron.min.js"></script>
    <script src="assets/js/browser.min.js"></script>
    <script src="assets/js/breakpoints.min.js"></script>
    <script src="assets/js/util.js"></script>
    <script src="assets/js/main.js"></script>

    



</body>
</html>