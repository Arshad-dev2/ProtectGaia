<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewBag.Title - Eco Morph</title>
    <!-- Favicon -->
    <link rel="shortcut icon" type="/image/icon" href="/favicon.ico" />
    <!-- Font Awesome -->
   @* <link href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">*@
    <link href="~/lib/font-awesome/css/all.min.css" rel="stylesheet" />
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/css/bootstrap.min.css" integrity="sha384-/Y6pD6FV/Vv2HJnA6t+vslU6fwYXjCFtcEpHbNJ0lyAFsXTsjBbfaDjzALeQsN6M" crossorigin="anonymous">
    <!-- Slick slider -->
    <link href="~/css/slick.css" rel="stylesheet">
    <!-- Gallery Lightbox -->
    <link href="~/css/magnific-popup.css" rel="stylesheet">
    <!-- Skills Circle CSS  -->
    <link rel="stylesheet" type="text/css" href="https://unpkg.com/circlebars@1.0.3/dist/circle.css">


    <!-- Main Style -->
    <link href="~/css/style.css" rel="stylesheet">

    <!-- Fonts -->
    <!-- Google Fonts Raleway -->
    <link href="https://fonts.googleapis.com/css?family=Raleway:300,400,400i,500,500i,600,700" rel="stylesheet">
    <!-- Google Fonts Open sans -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:400,400i,600,700,800" rel="stylesheet">

    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="/js/geoLocation.js"></script>

</head>
<body>

    <!--START SCROLL TOP BUTTON -->
    <a class="scrollToTop" href="#">
        <i class="fa fa-angle-up"></i>
    </a>

    <!-- END SCROLL TOP BUTTON -->
    <!-- Start Header -->

    <header id="mu-hero">

        <!--<img src="~/images/Home/ghg_home - Copy.jpg" alt="EcoMorph" class="title-image" style="width: 100%; height: auto" />-->


        <div class="new-container">
            <img src="~/images/Home/ghg_home - Copy.jpg" alt="EcoMorph" style=" filter: brightness(80%);opacity:0.9; width:100%; height:auto">
            <div class="image-text-block" >
                <p style=" font-size:42px; color:#f5f5f5; font-family:Comic Sans MS, cursive, sans-serif"><b> ECOMORPH </b></p>
              
                <p style=" font-size:38px; color:#f5f5f5; font-family:Comic Sans MS, cursive, sans-serif"/><b>Every Action makes a Difference!</b></p>

            </div>
        </div>

        <div class="top-right">
            <div class="deviceLocation"> <p>Detecting . . .</p></div>
            <nav class="navbar navbar-expand-lg navbar-light mu-navbar">
                <div>
                    <!-- Logo -->
                    <a asp-controller="Home" asp-action="Index"><img src="~/images/Home/eco_logo.png" alt="Eco Morph" style="height:70%;width:70%; padding-left: 50px;" /></a>
                    <!-- image based logo -->
                    <!-- <a class="navbar-brand mu-logo" href="index.html"><img src="assets/images/logo.png" alt="logo"></a> -->
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="fa fa-bars"></span>
                    </button>
                </div>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav mr-auto mu-navbar-nav" style="padding-right: 80px;">
                        <li class="nav-item active">
                            <a asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="dropdown-toggle" href="#" role="button" id="navbarDropdown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Explore</a>
                            <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                                <a class="dropdown-item" asp-controller="Explore" asp-Action="Infographics" onclick="MakeActive('infographics')">Infographics</a>
                                <a class="dropdown-item" asp-controller="Explore" asp-Action="Visualization">Visualizations</a>
                            </div>
                        </li>
                        <li class="nav-item" id="aboutusone"><a asp-controller="Blogs" asp-Action="Index" onclick="MakeActive('blogs')">Blogs</a></li>
                    </ul>
                </div>
            </nav>
        </div>
    </header>


    <!-- End Header -->
    <main>
        @RenderBody()
    </main>
    <!-- End main content -->
    <!-- Start footer -->

    <div class="row" style="height:50px;">

    </div>
    <footer>
        <div class="new-container">
            <img src="~/images/Home/1.jpg" alt="contact-us" style=" filter: brightness(50%);opacity:0.9; width:100%; height:13rem;">
            <div class="new-text-block">
                <h4>Contact Us</h4><br />
                <p><i class="far fa-envelope"></i> environs@gmail.com</p>
                <p><i class="fas fa-map-marker-alt"></i> Melbourne, Australia</p>
            </div>
        </div>
        <div class="mu-container mu-footer-top" style="background-color:#292929">
           
            <center> <p class="mu-copy-right">Designed by <a rel="nofollow" href="https://www.ecomorph.me">Environs</a>. All rights reserved.</p></center>
        </div>
    </footer>
    <!-- End footer -->
    <!-- JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/js/bootstrap.min.js" integrity="sha384-h0AbiXch4ZDo7tp9hKZ4TsHbi047NrKGLO3SEJAg45jXxnGIfYzk4Si90RDIqNm1" crossorigin="anonymous"></script>
    <!-- Slick slider -->
    <script type="text/javascript" src="/js/slick.min.js"></script>
    <!-- Progress Bar -->
    <script src="https://unpkg.com/circlebars@1.0.3/dist/circle.js"></script>
    <!-- Filterable Gallery js -->
    <script type="text/javascript" src="/js/jquery.filterizr.min.js"></script>
    <!-- Gallery Lightbox -->
    <script type="text/javascript" src="/js/jquery.magnific-popup.min.js"></script>
    <!-- Counter js
    <script type="text/javascript" src="/js/counter.js"></script>-->
    <!-- Ajax contact form  -->
    <script type="text/javascript" src="/js/app.js"></script>
    <script type="text/javascript" src="/js/header.js"></script>


    <!-- Custom js -->
    <script type="text/javascript" src="/js/custom.js"></script>



</body>
</html>